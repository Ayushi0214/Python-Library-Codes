    "def demo(value):\n",
    "    if value <= 100:\n",
    "        return \"Low\"\n",
    "    elif value >100 and value <= 200:\n",
    "        return \"Medium\"\n",
    "    else:\n",
    "        return \"High\"\n",
    "\n",
    "df[\"Quantity Type\"] = df[\"Quantity Sold\"].apply(demo)\n",
    "df"
